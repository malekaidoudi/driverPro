{
  "expo": {
    "name": "UpsDriver",
    "slug": "upsdriver",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icone/ups-logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.upsdriver.app",
      "deploymentTarget": "15.5",
      "config": {
        "googleMapsApiKey": "AIzaSyCTjfrPmdPuJN2v2WBXcfCFpo_hw6L0j0o"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "UpsDriver needs camera access to scan addresses and labels.",
        "NSSpeechRecognitionUsageDescription": "UpsDriver uses speech recognition to dictate addresses.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "UpsDriver uses your location for turn-by-turn navigation.",
        "NSLocationWhenInUseUsageDescription": "UpsDriver uses your location to show your position on the map.",
        "UIBackgroundModes": [
          "location",
          "audio"
        ],
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "waze",
          "maps"
        ]
      }
    },
    "android": {
      "package": "com.upsdriver.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCTjfrPmdPuJN2v2WBXcfCFpo_hw6L0j0o"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "intentFilters": [
        {
          "action": "android.intent.action.VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "geo"
            }
          ],
          "category": [
            "android.intent.category.DEFAULT",
            "android.intent.category.BROWSABLE"
          ]
        }
      ]
    },
    "web": {},
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow UpsDriver to use your location to optimize routes."
        }
      ],
      "expo-speech-recognition",
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "UpsDriver needs camera access to scan addresses and labels.",
          "enableMicrophonePermission": false,
          "enableFrameProcessors": true
        }
      ]
    ],
    "scheme": "upsdriver"
  }
}